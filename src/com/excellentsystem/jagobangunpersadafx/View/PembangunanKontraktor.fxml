<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="1024.0" styleClass="stage" stylesheets="@../Resource/MyTheme.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.excellentsystem.jagobangunpersadafx.View.PembangunanKontraktorController">
   <children>
      <GridPane hgap="5.0" vgap="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <TableView fx:id="pembangunanKontraktorTable" prefHeight="200.0" prefWidth="200.0" tableMenuButtonVisible="true" GridPane.rowIndex="1">
               <columns>
                  <TableColumn fx:id="noPembangunanColumn" maxWidth="1000.0" text="No Pembangunan" />
                  <TableColumn fx:id="kontraktorColumn" maxWidth="1000.0" prefWidth="120.0" text="Kontraktor" />
                  <TableColumn fx:id="propertyColumn" maxWidth="1000.0" prefWidth="120.0" text="Property" />
                  <TableColumn fx:id="totalPembangunanColumn" maxWidth="1000.0" prefWidth="75.0" text="Total Pembangunan" />
                  <TableColumn fx:id="pembayaranColumn" maxWidth="1000.0" prefWidth="100.0" text="Pembayaran" />
                  <TableColumn fx:id="sisaPembayaranColumn" maxWidth="1000.0" prefWidth="100.0" text="Sisa Pembayaran" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0" GridPane.rowIndex="2">
               <children>
                  <Label text="Total Pembangunan" />
                  <Label fx:id="totalPembangunanLabel" alignment="CENTER_RIGHT" style="-fx-font-weight: bold;" text="0" />
                  <Label layoutX="880.0" layoutY="17.0" text="Total Pembayaran" />
                  <Label fx:id="totalPembayaranLabel" alignment="CENTER_RIGHT" layoutX="997.0" layoutY="17.0" style="-fx-font-weight: bold;" text="0" />
                  <Label layoutX="880.0" layoutY="17.0" text="Total Sisa Pembayaran" />
                  <Label fx:id="totalSisaPembayaranLabel" alignment="CENTER_RIGHT" layoutX="997.0" layoutY="17.0" style="-fx-font-weight: bold;" text="0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                     <children>
                        <TextField fx:id="searchField" maxWidth="300.0" prefHeight="30.0" prefWidth="300.0" promptText="search..." styleClass="search-field" />
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </GridPane>
   </children>
</AnchorPane>
